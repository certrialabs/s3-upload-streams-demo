{
  "env": {
    "node": true,
    "amd": true,
    "es6": true
  },
  "globals": {},
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "destructuring": true,
    "forOf": true,
    "modules": true,
    "superInFunctions": true,
    "templateStrings": true
  },
  "parser": null,
  "rules": {
    "eol-last": 2,
    "eqeqeq": 2,
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "strict": 2,
    "array-bracket-spacing": 1,
    "comma-spacing": [
      1,
      {
        "after": true
      }
    ],
    "lines-around-comment": 1,
    "no-multiple-empty-lines": [
      1,
      {
        "max": 2
      }
    ],
    "semi": 2,
    "no-trailing-spaces": 1,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "no-mixed-spaces-and-tabs": 1,
    "brace-style": 1,
    "no-unused-vars": 1,
    "comma-style": [
      1,
      "last"
    ],
    "no-var": 2,
    "no-inline-comments": 1,
    "no-inner-declarations": 1,
    "arrow-spacing": [
      1,
      {
        "before": true,
        "after": true
      }
    ]
  }
}
